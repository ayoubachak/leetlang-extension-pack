{
  "name": "Leet",
  "displayName": "Leet",
  "description": "Syntax highlighting for the Leet programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "leet",
      "aliases": ["Leet Lang", "leet"],
      "extensions": [".lt",".leet"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "leet",
      "scopeName": "source.lt",
      "path": "./syntaxes/leet.tmLanguage.json"
    }]
  }
}
